package com.weatherapp.weatherentities;

public class FutureDay {

	private String date;

	private Day day;

	public FutureDay(String date, Day day) {
		super();
		this.date = date;
		this.day = day;
	}

	public FutureDay() {
		super();
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public Day getDay() {
		return day;
	}

	public void setDay(Day day) {
		this.day = day;
	}

	@Override
	public String toString() {
		return "FutureDay [date=" + date + ", day=" + day + "]";
	}


}
